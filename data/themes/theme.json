{
    "@title":{
        "font":{
            "name":"Helvetica", 
            "size":"50"
        }, 
        "colours":{
            "normal_text":"rgb(255,255,255)",
            "dark_bg":"rgb(50,50,50)", 
            "text_shadow":"rgb(0,0,0)"
        }, 
        "misc":{
            "text_shadow":"1",
            "text_shadow_size":"2", 
            "text_shadow_offset":"3,3"
        }
    }, 
    "@title2":{
        "font":{
            "name":"Helvetica", 
            "size":"40"
        }
    },
    "@title3":{
        "font":{
            "name":"Helvetica", 
            "size":"30"
        }
    },
    "@label":{
        "font":{
            "name":"Helvetica", 
            "size":"20"
        }
    },
    "@error_msg":{
        "font":{
            "name":"Helvetica", 
            "size":"20"
        },
        "colours":{
            "normal_text":"rgb(255,0,0)"
        }
    }, 
    "@left_error_msg":{
        "font":{
            "name":"Helvetica", 
            "size":"20"
        },
        "colours":{
            "normal_text":"rgb(255,0,0)"
        }, 
        "misc":{
            "text_horiz_alignment":"left"
        }
    }, 
    "button":{
        "font":{
            "name":"Helvetica", 
            "size":"20"
        }
    }, 
    "@confirm_btn":{
        "colours":{
            "normal_bg":"rgb(0, 255, 0)", 
            "hovered_bg":"rgb(50, 100, 50)", 
            "normal_text":"rgb(0, 0, 0)"
        }
    }, 
    "selection_list.@selection_list_item":{
        "misc":{
            "border_width":"1"
        }
    }, 
    "@export_info_label":{
        "misc":{
            "text_horiz_alignment":"left"
        }
    }, 
    "#scene1_instructions":{
        "font":{
            "size":"20", 
            "name":"Helvetica"
        }
    }, 
    "#file_path_label":{
        "font":{
            "name":"Helvetica", 
            "size":"15"
        }
    }, 
    "#panning_msg":{
        "misc":{
            "text_horiz_alignment":"left"
        }
    }, 
    "#panning_image_name_label":{
        "misc":{
            "text_horiz_alignment":"right"
        }
    }, 
    "#export_error_label":{
        "colours":{
            "normal_text":"rgb(255,0,0)"
        }
    }, 
    "#export_queue_label":{
        "font":{
            "name":"Helvetica", 
            "size":"17"
        }
    }, 
    "#export_queue_tooltip_button":{
        "font":{
            "name":"Helvetica", 
            "size":"17"
        }, 
        "colours":{
            "normal_bg":"rgb(50,50,50)", 
            "hovered_bg":"rgb(50,50,50)", 
            "disabled_bg":"rgb(50,50,50)", 
            "selected_bg":"rgb(50,50,50)", 
            "active_bg":"rgb(50,50,50)"
        }, 
        "misc":{
            "border_width":"0"
        }
    }, 
    "#export_queue_button":{
        "font":{
            "name":"Helvetica", 
            "size":"17"
        }
    }, 
    "#export_queue_box":{
        "colours":{
            "dark_bg":"rgb(50,50,50)"
        }, 
        "misc":{
            "border_width":"0"
        }
    }, 
    "#export_queue_display":{
        "misc":{
            "border_width":"0"
        }
    }
}